{
	"name": "vscode-generator-eslint-prettier-example",
	"displayName": "vscode-generator-eslint-prettier-example",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
